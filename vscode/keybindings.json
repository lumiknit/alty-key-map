    },
    {
        "key": "alt+.",
        "command": "cursorPageDown",
        "when": "textInputFocus"
    },
    {
        "key": "alt+/",
        "command": "cursorBottom",
        "when": "textInputFocus"
    },
    {
        "key": "alt+\\",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+\\",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "alt+a",
        "command": "runCommands",
        "args": {
            "commands": [
                "cursorLineStart",
                "cursorLineEndSelect"
            ]
        }
    },
    {
        "key": "alt+b",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
